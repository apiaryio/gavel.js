{
  "name": "gavel",
  "version": "0.0.0-semantically-released",
  "description": "Validator of HTTP transactions (JavaScript implementation)",
  "main": "lib/gavel.js",
  "engines": {
    "node": ">= 8"
  },
  "bin": {
    "gavel": "bin/gavel"
  },
  "scripts": {
    "test": "npm run test:server && npm run test:browser && npm run test:features",
    "test:server": "mocha \"test/unit/**/*-test.js\"",
    "test:browser": "mochify \"test/unit/**/*.js\"",
    "test:features": "node scripts/cucumber.js",
    "coverage": "scripts/cov",
    "coveralls": "npm run coverage mocha-lcov-reporter | coveralls",
    "ci:lint": "npm run lint",
    "ci:test": "npm run test",
    "ci:release": "semantic-release"
  },
  "dependencies": {
    "amanda": "1.0.1",
    "async": "2.6.1",
    "caseless": "0.12.0",
    "clone": "2.1.2",
    "commander": "2.20.0",
    "content-type": "1.0.4",
    "curl-trace-parser": "0.0.10",
    "deep-equal": "1.0.1",
    "googlediff": "0.1.0",
    "http-string-parser": "0.0.6",
    "json-parse-helpfulerror": "1.0.3",
    "json-pointer": "0.6.0",
    "media-typer": "1.0.2",
    "tv4": "1.3.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "cross-spawn": "6.0.5",
    "cucumber": "1.3.2",
    "gavel-spec": "1.2.1",
    "jscoverage": "0.6.0",
    "mocha": "6.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "mochify": "6.2.0",
    "nyc": "13.3.0",
    "prettyjson": "1.2.1",
    "semantic-release": "15.12.4",
    "sinon": "7.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiaryio/gavel.js"
  },
  "keywords": [
    "http",
    "validation",
    "diff",
    "request",
    "response",
    "gavel"
  ],
  "author": "Apiary Czech Republic, s.r.o. <support@apiary.io>",
  "license": "MIT"
}
